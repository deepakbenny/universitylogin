{
  "manifest_version": 2,

  "name": "christlogin",

   "background": {
    "scripts": ["popup.js","background.js"]
  },

  "description": "quick way to login",

  "options_page": "options.html",

  "version": "0.0.1",

  "icons": { "16": "icon.png",
           "48": "icon.png",
          "128": "icon.png" },

  "permissions": [
    "http://192.168.100.100/login.xml/",
    "storage"
  ],

  "browser_action": {
    "default_icon": "icon.png"
  }

}